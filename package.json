{
  "name": "unplugin-class-extractor",
  "type": "module",
  "version": "0.0.9",
  "packageManager": "pnpm@7.33.7",
  "description": "A powerful unplugin for extracting class and className attributes from specified file types, generating a new string for Tailwind CSS style extraction.",
  "author": "Simon He",
  "license": "MIT",
  "funding": "https://github.com/sponsors/Simon-He95",
  "homepage": "https://github.com/Simon-He95/unplugin-class-extractor#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Simon-He95/unplugin-class-extractor.git"
  },
  "bugs": {
    "url": "https://github.com/Simon-He95/unplugin-class-extractor/issues",
    "email": "support@example.com"
  },
  "keywords": [
    "unplugin",
    "class-extractor",
    "tailwindcss",
    "plugin",
    "build-tool",
    "vite",
    "webpack",
    "rollup",
    "esbuild",
    "rspack",
    "nuxt",
    "astro",
    "css",
    "className",
    "automation"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./astro": {
      "types": "./dist/astro.d.ts",
      "import": "./dist/astro.js",
      "require": "./dist/astro.cjs"
    },
    "./rspack": {
      "types": "./dist/rspack.d.ts",
      "import": "./dist/rspack.js",
      "require": "./dist/rspack.cjs"
    },
    "./vite": {
      "types": "./dist/vite.d.ts",
      "import": "./dist/vite.js",
      "require": "./dist/vite.cjs"
    },
    "./webpack": {
      "types": "./dist/webpack.d.ts",
      "import": "./dist/webpack.js",
      "require": "./dist/webpack.cjs"
    },
    "./rollup": {
      "types": "./dist/rollup.d.ts",
      "import": "./dist/rollup.js",
      "require": "./dist/rollup.cjs"
    },
    "./esbuild": {
      "types": "./dist/esbuild.d.ts",
      "import": "./dist/esbuild.js",
      "require": "./dist/esbuild.cjs"
    },
    "./nuxt": {
      "types": "./dist/nuxt.d.ts",
      "import": "./dist/nuxt.js",
      "require": "./dist/nuxt.cjs"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.js",
      "require": "./dist/types.cjs"
    },
    "./*": "./*"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch src",
    "build:fix": "esno scripts/postbuild.ts",
    "lint": "eslint . --cache",
    "lint:fix": "npm run lint --fix",
    "prepublishOnly": "npm run build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "@nuxt/kit": "^3",
    "@nuxt/schema": "^3",
    "esbuild": "*",
    "rollup": "^3",
    "vite": ">=3",
    "webpack": "^4 || ^5"
  },
  "peerDependenciesMeta": {
    "@nuxt/kit": {
      "optional": true
    },
    "@nuxt/schema": {
      "optional": true
    },
    "esbuild": {
      "optional": true
    },
    "rollup": {
      "optional": true
    },
    "vite": {
      "optional": true
    },
    "webpack": {
      "optional": true
    }
  },
  "dependencies": {
    "unplugin": "^1.16.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.19.0",
    "@nuxt/kit": "^3.17.7",
    "@nuxt/schema": "^3.17.7",
    "@rollup/pluginutils": "^5.2.0",
    "@types/node": "^18.19.120",
    "bumpp": "^8.2.1",
    "eslint": "^9.31.0",
    "esno": "^0.16.3",
    "fast-glob": "^3.3.3",
    "lint-staged": "^13.3.0",
    "picocolors": "^1.1.1",
    "rollup": "^4.45.1",
    "tsup": "^6.7.0",
    "tsx": "^3.14.0",
    "typescript": "^4.9.5",
    "vite": "^5.4.19",
    "vitest": "^0.15.2",
    "webpack": "^5.100.2"
  },
  "lint-staged": {
    "*": [
      "prettier --write --cache --ignore-unknown"
    ],
    "*.{vue,js,ts,jsx,tsx,md,json}": "eslint --fix"
  }
}
